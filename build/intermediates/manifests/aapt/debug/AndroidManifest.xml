<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.askzhao.dp.android.sdk"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="9"
        android:targetSdkVersion="23"
        tools:ignore="GradleOverrides" />

    <!-- 同步数据需要网络权限 -->
    <uses-permission android:name="android.permission.INTERNET" />
    <!-- 获取网络状态 -->
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <!-- 获取运营商信息 -->
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />

    <application>
        <provider
            android:name="com.askzhao.dp.android.sdk.SensorsDataContentProvider"
            android:authorities="dollar_openBracket_applicationId_closeBracket.SensorsDataContentProvider"
            android:enabled="true"
            android:exported="false" />

        <meta-data
            android:name="com.sensorsdata.analytics.android.MainProcessName"
            android:value="dollar_openBracket_applicationId_closeBracket" />
    </application>

</manifest>